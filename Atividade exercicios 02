programa {
  
  funcao inicio() {
  	
    inteiro vetor[10] = {2,5,1,3,4,9,7,8,10,6}, tamanho = 9
	
	para (inteiro y = tamanho; y > 0; y--){
		para (inteiro x = tamanho; x > 0; x--){
			se (vetor[x] > vetor[x - 1]){
				inteiro copia = vetor[x]
				vetor[x] = vetor[x-1]
				vetor[x-1] = copia
			}
		}
	}
	
	
	escreva("\n", "vetor ordenado", "\n")
	
	para (inteiro i = 0; i <= tamanho; i++){
		
      escreva(vetor[i], " ")
	  
    }
  }
}